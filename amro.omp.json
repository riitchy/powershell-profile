{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\uf508 {{ .UserName }}@{{ .HostName }} ",
          "foreground": "#45F1C2",
          "type": "session",
          "style": "plain"
        },
        {
          "template": "-{{ .Shell }}-",
          "foreground": "#B388FF",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": " \uf07b {{ .Path }} ",
          "foreground": "#0CA0D8",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\ue3bf ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "template": "‚ùØ ",
          "foreground": "#CD4277",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
